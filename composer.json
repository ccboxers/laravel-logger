{
    "name": "layman/laravel-logger",
    "description": "laravel logger log logging file database",
    "license": "MIT",
    "keywords": [
        "laravel",
        "logger",
        "log",
        "logging",
        "file",
        "database"
    ],
    "autoload": {
        "psr-4": {
            "Layman\\LaravelLogger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "layman"
        }
    ],
    "require": {
        "php": "^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Layman\\LaravelLogger\\LoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
